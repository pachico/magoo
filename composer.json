{
    "name": "pachico/magoo",
    "type": "library",
    "description": "PHP library to mask (redact) credit card numbers, emails and more.",
    "license": "MIT",
    "keywords": [
        "Mask",
        "Masking",
        "Sensitive",
        "Credit card",
        "Email",
        "Security",
        "Obfuscation",
        "Redact",
        "Bleep out"

    ],
    "authors": [
        {
            "name": "Mariano F.co BenÃ­tez Mulet",
            "email": "pachicodev@gmail.com",
            "homepage": "https://github.com/pachico",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Pachico\\Magoo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pachico\\MagooTest\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "psr/log": "^1.0"
    },
    "scripts": {
        "test": "phpunit",
        "sniff": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src/ tests/",
        "format": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src/ tests/"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "codacy/coverage": "dev-master",
        "squizlabs/php_codesniffer": "^2.7"
    },
    "config": {
        "sort-packages": true
    }
}
